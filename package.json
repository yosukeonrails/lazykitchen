{
  "name": "lazykitchen",
  "version": "0.0.1",
  "description": "A React template",
  "main": "js/index.js",
  "scripts": {
    "start": "supervisor server.js",
    "dev": "run-p watch serve",
    "test": "mocha --recursive --compilers js:babel-core/register",
    "build": "run-s build:*",
    "clean": "rimraf build",
    "serve": "npm start",
    "watch": "run-p watch:*",
    "mkdir": "mkdirp build",
    "clean:html": "rimraf build/index.html",
    "mkdir:html": "run-s mkdir",
    "build:html": "run-s clean:html mkdir:html _build:html",
    "_build:html": "cpr index.html build/index.html",
    "watch:html": "run-s build:html _watch:html",
    "_watch:html": "chokidar index.html -c \"npm run build:html\"",
    "clean:js": "rimraf build/js",
    "mkdir:js": "mkdirp build/js",
    "build:js": "run-s clean:js mkdir:js _build:js",
    "_build:js": "webpack",
    "watch:js": "run-s build:js _watch:js",
    "_watch:js": "webpack --watch",
    "clean:css": "rimraf build/css",
    "mkdir:css": "run-s mkdir",
    "build:css": "run-s clean:css mkdir:css _build:css",
    "_build:css": "lessc --clean-css --source-map css/index.less build/lazykitchen.0.0.1.min.css",
    "watch:css": "run-s build:css _watch:css",
    "_watch:css": "chokidar css/**/* -c \"npm run build:css\"",
    "mkdir:assets": "run-s mkdir",
    "clean:assets": "rimraf build/assets",
    "build:assets": "run-s clean:assets mkdir:assets _build:assets",
    "_build:assets": "cpr assets build/assets",
    "watch:assets": "run-s build:assets _watch:assets",
    "_watch:assets": "chokidar assets/**/* -c \"npm run build:assets\""
  },
  "author": "Joe Turner <joe@oampo.co.uk>",
  "license": "UNLICENSED",
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.13.1",
    "babel-preset-react": "^6.11.1",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.5.1",
    "chai-http": "^3.0.0",
    "chokidar-cli": "^1.2.0",
    "cpr": "^2.0.0",
    "enzyme": "^2.4.1",
    "http-server": "^0.9.0",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "mkdirp": "^0.5.1",
    "mocha": "^3.0.1",
    "npm-run-all": "^3.0.0",
    "passport": "^0.3.2",
    "passport-http": "^0.3.0",
    "rimraf": "^2.5.4",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "webpack": "^1.13.1"
  },
  "dependencies": {
    "amazon-product-api": "^0.4.2",
    "bcrypt-nodejs": "0.0.3",
    "bcryptjs": "^2.3.0",
    "body-parser": "^1.17.1",
    "cookie-parser": "^1.4.3",
    "es6-promise": "^4.0.5",
    "express": "^4.14.0",
    "hammerjs": "^2.0.8",
    "isomorphic-fetch": "^2.2.1",
    "jquery-hammerjs": "^2.0.0",
    "less": "^2.7.2",
    "less-plugin-clean-css": "^1.5.1",
    "mongodb": "^2.2.10",
    "mongoose": "^4.6.3",
    "passport-facebook": "^2.1.1",
    "passport-local": "^1.0.0",
    "react": "^15.3.1",
    "react-bootstrap": "^0.30.7",
    "react-dom": "^15.3.1",
    "react-hammerjs": "^0.5.0",
    "react-phone-input": "^1.1.3",
    "react-redux": "^4.4.5",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.8",
    "redux": "^3.6.0",
    "redux-pack": "^0.1.4",
    "redux-thunk": "^2.1.0",
    "sendgrid": "^4.9.0",
    "supervisor": "^0.12.0"
  }
}
